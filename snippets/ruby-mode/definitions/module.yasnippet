# name: module ... end
# key: module
# --
module ${1:`(let ((fn (capitalize (file-name-nondirectory
                                 (file-name-sans-extension
				 (or (buffer-file-name)
				     (buffer-name (current-buffer))))))))
           (cond
             ((string-match "_" fn) (yas-underscore-to-camel fn))
              (t fn)))`}
  $0
end
